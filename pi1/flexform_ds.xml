<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<ROOT>
		<type>array</type>
		<el>
			<thumbnailWidth>
				<TCEforms>
					<label>LLL:EXT:album3x/pi1/locallang.xml:thumbnail.width</label>
					<config>
						<type>input</type>
						<size>5</size>
						<max>3</max>
						<default>120</default>
						<eval>trim,int,num</eval>
						<range>
							<lower>20</lower>
							<upper>500</upper>
						</range>
					</config>
				</TCEforms>
			</thumbnailWidth>
			<thumbnailHeight>
				<TCEforms>
					<label>LLL:EXT:album3x/pi1/locallang.xml:thumbnail.height</label>
					<config>
						<type>input</type>
						<size>5</size>
						<max>3</max>
						<default>90</default>
						<eval>trim,int,num</eval>
						<range>
							<lower>20</lower>
							<upper>500</upper>
						</range>
					</config>
				</TCEforms>
			</thumbnailHeight>
			<midImageWidth>
				<TCEforms>
					<label>LLL:EXT:album3x/pi1/locallang.xml:mid.image.width</label>
					<config>
						<type>input</type>
						<size>5</size>
						<max>3</max>
						<default>450</default>
						<eval>trim,int,num</eval>
						<range>
							<lower>20</lower>
							<upper>500</upper>
						</range>
					</config>
				</TCEforms>
			</midImageWidth>
			<midImageHeight>
				<TCEforms>
					<label>LLL:EXT:album3x/pi1/locallang.xml:mid.image.height</label>
					<config>
						<type>input</type>
						<size>5</size>
						<max>3</max>
						<default>335</default>
						<eval>trim,int,num</eval>
						<range>
							<lower>20</lower>
							<upper>500</upper>
						</range>
					</config>
				</TCEforms>
			</midImageHeight>
			<columns>
				<TCEforms>
					<label>LLL:EXT:album3x/pi1/locallang.xml:cols</label>
					<config>
						<type>input</type>
						<size>5</size>
						<max>3</max>
						<eval>required,trim,int,num</eval>
						<range>
							<lower>1</lower>
						</range>
						<default>4</default>
					</config>
				</TCEforms>
			</columns>
			<rows>
				<TCEforms>
					<label>LLL:EXT:album3x/pi1/locallang.xml:rows</label>
					<config>
						<type>input</type>
						<size>5</size>
						<max>3</max>
						<eval>required,trim,int,num</eval>
						<range>
							<lower>1</lower>
						</range>
						<default>3</default>
					</config>
				</TCEforms>
			</rows>
			<storagePid>
				<TCEforms>
					<label>LLL:EXT:album3x/pi1/locallang.xml:storage.page</label>
					<config>
						<type>group</type>
						<internal_type>db</internal_type>
						<allowed>pages</allowed>
						<size>1</size>
						<minitems>1</minitems>
						<maxitems>1</maxitems>
						<autoSizeMax>3</autoSizeMax>
					</config>
				</TCEforms>
			</storagePid>
			<templateFile>
				<TCEforms>
					<label>LLL:EXT:album3x/pi1/locallang.xml:template.file</label>
					<config>
						<type>input</type>
						<eval>trim,required</eval>
						<default>EXT:album3x/res/pi1.html</default>
						<wizards type="array">
							<_PADDING>2</_PADDING>
							<link type="array">
								<type>popup</type>
								<title>Link</title>
								<icon>link_popup.gif</icon>
								<script>browse_links.php?mode=file</script>
								<JSopenParams>height=300,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
							</link>
						</wizards>
					</config>
				</TCEforms>
			</templateFile>
		</el>
	</ROOT>
</T3DataStructure>